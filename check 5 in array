        <?php
            $arr = [];
            
            for ($i = 0; $i <= 9; $i++) {
                $arr[$i] = random_int(0, 9);
            }
            
            $flag = false;
            foreach ($arr as $element) {
                if ($element === 5) {
                    $flag = true;
                }
            }
            
            if ($flag === true) {
                echo 'Да';
            }
            else {
                echo 'Нет';
            }
            
            var_dump($arr);
        ?>
    </body>
